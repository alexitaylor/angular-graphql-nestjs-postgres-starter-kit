{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexitaylor/angular-graphql-nestjs-postgres-starter-kit"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "serve": "npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "keywords": [
    "angular-graphql-express-postgres-starter-kit",
    "angular",
    "Angular 8",
    "graphql",
    "server",
    "node",
    "express",
    "graphql",
    "apollo",
    "postgres",
    "boilerplate"
  ],
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "@angular/service-worker": "8.0.0",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@ng-bootstrap/ng-bootstrap": "4.2.1",
    "@ngx-translate/core": "11.0.1",
    "@toverux/ngx-sweetalert2": "4.0.0",
    "@types/date-fns": "2.6.0",
    "@types/google-libphonenumber": "7.4.17",
    "@types/graphql": "14.2.0",
    "@types/ws": "6.0.1",
    "angulartics2": "7.5.2",
    "apollo-angular": "1.6.0",
    "apollo-angular-link-http": "1.7.0",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.2",
    "apollo-link-context": "1.0.17",
    "apollo-link-error": "1.1.10",
    "apollo-link-ws": "1.0.17",
    "bootstrap": "4.3.1",
    "core-js": "3.1.3",
    "date-fns": "1.30.1",
    "google-libphonenumber": "3.2.2",
    "graphql": "14.3.1",
    "graphql-tag": "2.10.1",
    "lodash": "4.17.13",
    "rxjs": "6.5.2",
    "subscriptions-transport-ws": "0.9.16",
    "sweetalert2": "8.11.7",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.1",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ngx-rocket/scripts": "3.0.3",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.133",
    "@types/node": "12.0.5",
    "codelyzer": "5.1.0",
    "hads": "1.7.3",
    "htmlhint": "0.11.0",
    "https-proxy-agent": "2.2.1",
    "husky": "2.4.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-junit-reporter": "1.2.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "protractor": "5.4.2",
    "puppeteer": "1.17.0",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.8.0",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
