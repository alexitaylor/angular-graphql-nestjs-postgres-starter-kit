{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexitaylor/angular-graphql-nestjs-postgres-starter-kit"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "serve": "npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "keywords": [
    "angular-graphql-express-postgres-starter-kit",
    "angular",
    "angular 7",
    "graphql",
    "server",
    "node",
    "express",
    "graphql",
    "apollo",
    "postgres",
    "boilerplate"
  ],
  "dependencies": {
    "@angular/animations": "7.0.2",
    "@angular/common": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/core": "7.0.2",
    "@angular/forms": "7.0.2",
    "@angular/http": "7.0.2",
    "@angular/platform-browser": "7.0.2",
    "@angular/platform-browser-dynamic": "7.0.2",
    "@angular/router": "7.0.2",
    "@angular/service-worker": "7.0.2",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@ng-bootstrap/ng-bootstrap": "3.3.0",
    "@ngx-translate/core": "10.0.2",
    "@toverux/ngx-sweetalert2": "4.0.0",
    "@types/date-fns": "2.6.0",
    "@types/google-libphonenumber": "7.4.17",
    "@types/graphql": "14.0.3",
    "@types/ws": "6.0.1",
    "angulartics2": "6.3.1",
    "apollo-angular": "1.5.0",
    "apollo-angular-link-http": "1.4.0",
    "apollo-cache-inmemory": "1.3.9",
    "apollo-client": "2.4.5",
    "apollo-link-context": "1.0.9",
    "apollo-link-error": "1.1.1",
    "apollo-link-ws": "1.0.9",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "date-fns": "1.30.1",
    "google-libphonenumber": "3.1.15",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "lodash": "4.17.11",
    "rxjs": "6.3.3",
    "subscriptions-transport-ws": "0.9.15",
    "sweetalert2": "7.29.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.0.2",
    "@angular/language-service": "7.0.2",
    "@angular-devkit/build-angular": "0.13.8",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ngx-rocket/scripts": "3.0.1",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/lodash": "4.14.117",
    "@types/node": "8.10.37",
    "codelyzer": "4.5.0",
    "hads": "1.7.1",
    "htmlhint": "0.10.1",
    "https-proxy-agent": "2.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-junit-reporter": "1.2.0",
    "prettier": "1.14.3",
    "tslint-config-prettier": "1.15.0",
    "stylelint-config-prettier": "4.0.0",
    "pretty-quick": "1.8.0",
    "husky": "0.14.3",
    "protractor": "5.4.1",
    "puppeteer": "1.10.0",
    "stylelint": "9.6.0",
    "stylelint-config-recommended-scss": "3.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.3.2",
    "ts-node": "7.0.1",
    "tslint": "5.16.0",
    "typescript": "3.1.6"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
